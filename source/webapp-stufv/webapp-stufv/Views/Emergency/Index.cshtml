@section Content{
@model IEnumerable<webapp_stufv.Models.Emergency>

<div class="page-header"><h1>@ViewBag.Title</h1></div>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TelNr)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TelNr)
        </td>
    </tr>
}

</table>
    }
